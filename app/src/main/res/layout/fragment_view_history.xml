<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="421dp"
        android:layout_height="64dp"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" >
        <Button
            android:id="@+id/b"
            android:layout_width="78dp"
            android:layout_height="50dp"
            android:text="BACK"
            android:textColor="#8253D6"
            app:backgroundTint="@color/white"
            android:radius="90dp"
            app:iconTint="#8253D6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.066"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023" />
        <TextView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="@string/history"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:fontFamily="sans-serif"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout"
            app:layout_constraintStart_toStartOf="@+id/linearLayout"
            app:layout_constraintTop_toTopOf="@+id/linearLayout"
            android:gravity="center_horizontal"
            android:layout_marginStart="30dp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/l1"
        android:layout_width="421dp"
        android:layout_height="667dp"
        android:layout_marginTop="16dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linearLayout"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:id="@+id/l2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/l1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/l1"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintVertical_bias="0.0">

            <Button
                android:id="@+id/button"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"
                android:fontFamily="sans-serif"
                android:gravity="fill_horizontal"
                android:text="23/3/2019"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/l2"
                app:layout_constraintEnd_toEndOf="@id/l2"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@id/l2"
                app:layout_constraintTop_toBottomOf="@id/l2"
                app:layout_constraintVertical_bias="0.0"


                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:layout_marginLeft="270dp"
                android:src="@drawable/arr1"
                app:layout_constraintBottom_toBottomOf="@id/l2"
                app:layout_constraintEnd_toEndOf="@id/l2"
                app:layout_constraintStart_toStartOf="@id/l2"
                app:layout_constraintTop_toBottomOf="@id/l2"
                android:layout_marginStart="270dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/l3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/l1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/l1"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintVertical_bias="0.0">

            <Button
                android:id="@+id/button1"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"
                android:fontFamily="sans-serif"
                android:gravity="fill_horizontal"
                android:text="@string/_10_6_2019"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/l3"
                app:layout_constraintEnd_toEndOf="@id/l3"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@id/l3"
                app:layout_constraintTop_toBottomOf="@id/l3"
                app:layout_constraintVertical_bias="0.0"


                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:layout_marginLeft="270dp"
                android:src="@drawable/arr1"
                app:layout_constraintBottom_toBottomOf="@id/l3"
                app:layout_constraintEnd_toEndOf="@id/l3"
                app:layout_constraintStart_toStartOf="@id/l3"
                app:layout_constraintTop_toBottomOf="@id/l3"/>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/l4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/l1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/l1"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintVertical_bias="0.0">

            <Button
                android:id="@+id/button3"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:backgroundTint="@color/white"
                android:fontFamily="sans-serif"
                android:gravity="fill_horizontal"
                android:text="10/6/2019"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="@id/l4"
                app:layout_constraintEnd_toEndOf="@id/l4"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@id/l4"
                app:layout_constraintTop_toBottomOf="@id/l4"
                app:layout_constraintVertical_bias="0.0"


                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="50dp"
                android:layout_height="35dp"
                android:layout_marginLeft="270dp"
                android:src="@drawable/arr1"
                app:layout_constraintBottom_toBottomOf="@id/l4"
                app:layout_constraintEnd_toEndOf="@id/l4"
                app:layout_constraintStart_toStartOf="@id/l4"
                app:layout_constraintTop_toBottomOf="@id/l4">

            </androidx.appcompat.widget.AppCompatImageView>

        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>