    <LinearLayout
        android:padding="15dp"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

            <TextView
                android:gravity="center"
                android:id="@+id/historyDate"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="18sp" />


        <TextView
            android:textSize="20sp"
            android:lines="2"
            android:text="We have evaluated your responses.Our test results show that you belong to:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </TextView>

            <TextView
                android:layout_marginTop="20dp"
                android:gravity="center"
                android:id="@+id/testnameTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#8253D6"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/historyDate"
                app:layout_constraintVertical_bias="0.046" />

    </LinearLayout>
    <LinearLayout

        android:weightSum="6"
        android:orientation="vertical"
        android:gravity="center"
        android:padding="15dp"

        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/HistrynormalText"
            android:layout_marginBottom="25dp"
            android:layout_weight="1"
            android:text="Normal"
            android:gravity="center"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:layout_width="300dp"
            android:layout_height="0dp">
        </TextView>

        <TextView
            android:id="@+id/HistrymildText"
            android:layout_marginBottom="25dp"
            android:layout_weight="1"
            android:text="Mild"
            android:gravity="center"
            android:textSize="20sp"
            android:textColor="@color/black"

            android:layout_width="300dp"
            android:layout_height="0dp">
        </TextView>
        <TextView
            android:id="@+id/HistryModeratorText"
            android:layout_marginBottom="25dp"
            android:layout_weight="1"
            android:text="Moderate"
            android:gravity="center"
            android:textSize="20sp"
            android:textColor="@color/black"

            android:layout_width="300dp"
            android:layout_height="0dp">
        </TextView>
        <TextView
            android:id="@+id/HistryseverText"
            android:layout_marginBottom="25dp"
            android:layout_weight="1"
            android:text="Severe"
            android:gravity="center"
            android:textSize="20sp"
            android:textColor="@color/black"

            android:layout_width="300dp"
            android:layout_height="0dp">
        </TextView>
        <TextView
            android:id="@+id/HistryextremeText"
            android:layout_marginBottom="25dp"
            android:layout_weight="1"
            android:text="Extremely Severe"
            android:gravity="center"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:layout_width="300dp"
            android:layout_height="0dp">
        </TextView>

        <Button
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:id="@+id/History_therapyList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contact Therapist"
            android:textStyle="bold"
            app:backgroundTint="#8253D6" />


    </LinearLayout>



